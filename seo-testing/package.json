{
  "name": "kps-interier-seo-testing",
  "version": "1.0.0",
  "description": "Comprehensive SEO validation and testing suite for KPS Interiéry",
  "type": "module",
  "main": "seo-audit.js",
  "scripts": {
    "audit": "node seo-audit.js",
    "meta": "node meta-validator.js",
    "schema": "node schema-validator.js",
    "sitemap": "node sitemap-checker.js",
    "social": "node social-preview-tester.js",
    "monitor": "node performance-monitor.js monitor",
    "report": "node performance-monitor.js report",
    "test": "npm run audit https://kps-interiery.cz",
    "test:local": "npm run audit http://localhost:5173",
    "test:all": "npm run meta && npm run schema && npm run social && npm run sitemap",
    "monitor:daily": "npm run monitor https://kps-interiery.cz",
    "export:data": "node performance-monitor.js export json seo-data.json",
    "export:csv": "node performance-monitor.js export csv seo-data.csv"
  },
  "keywords": [
    "seo",
    "testing",
    "validation",
    "meta-tags",
    "schema-markup",
    "open-graph",
    "twitter-cards",
    "sitemap",
    "performance",
    "audit",
    "kps-interier",
    "furniture",
    "czech"
  ],
  "author": "KPS Interiéry SEO Team",
  "license": "MIT",
  "dependencies": {
    "jsdom": "^23.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/kps-interier"
  },
  "bugs": {
    "url": "https://github.com/your-repo/kps-interier/issues"
  },
  "homepage": "https://kps-interiery.cz"
}